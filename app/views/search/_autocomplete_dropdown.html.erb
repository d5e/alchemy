<div id="psf-autocomplete-dropdown">
    <% if autocomplete_dropdown && (last_id = autocomplete_dropdown.to_a.last.last rescue nil) %>
        <% autocomplete_dropdown.each do |s,id| %>
            <%= link_to s, resource_path(id), class: "btn" %>
            <% unless last_id == id %>
                <hr />
            <% end %>
        <% end %>
    <% end %>
</div>
